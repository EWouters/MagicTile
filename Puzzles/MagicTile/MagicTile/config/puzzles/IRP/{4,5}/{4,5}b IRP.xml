<PuzzleConfigClass>
   <ClassDisplayName>{4,5}b 30-Color</ClassDisplayName>
   <ClassID>IRP.{4,5}b.30</ClassID>
   <ExpectedNumColors>30</ExpectedNumColors>
   <IRPConfig>
      <DataFile>4_5b_cell.wrl</DataFile>
      <FirstTile>0</FirstTile>
      <Reflect>true</Reflect>
   </IRPConfig>
   <Identifications>
      <!--Green - Yellow-->
      <Identification>
         <!--around the horn-->
         <EdgeSet>1:2:1:2:2:2:1:2:3</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!--around the horn other direction-->
         <EdgeSet>1:2:3:2:2:2:3:2:3</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!--across the bay-->
         <EdgeSet>1:2:2:2:2:2:1</EdgeSet>
         <EndRotation>2</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!---->
         <EdgeSet>1:2:1:2:3:2:3</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!--filling in-->
         <EdgeSet>1:2:2:3:2:3:2:2:3</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!--filling in-->
         <EdgeSet>3:2:2:1:2:1:2:2:1</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>0:2</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <!--Blue - Red-->
      <Identification>
         <!--around the horn-->
         <EdgeSet>1:2:2:2:2:2:1</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <InitialEdgeSet>1:3</InitialEdgeSet>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <!--All-->
      <Identification>
         <!--around the horn or inside the box-->
         <EdgeSet>2:2:2</EdgeSet>
         <EndRotation>0</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
      <Identification>
         <!--Z-->
         <EdgeSet>2:1:2</EdgeSet>
         <EndRotation>2</EndRotation>
         <InPlaceReflection>false</InPlaceReflection>
         <UseMirroredEdgeSet>true</UseMirroredEdgeSet>
      </Identification>
   </Identifications>
   <NumTiles>7500</NumTiles>
   <P>4</P>
   <Q>5</Q>
   <Specific>
      <Puzzle>
         <SlicingCircles>
            <EdgeCentered/>
            <FaceCentered>
               <Distance>0:0:1:0</Distance>
            </FaceCentered>
            <Thickness>0.02</Thickness>
            <VertexCentered/>
         </SlicingCircles>
      </Puzzle>
      <Puzzle>
         <SlicingCircles>
            <EdgeCentered/>
            <FaceCentered>
               <Distance>0:2:0:0</Distance>
            </FaceCentered>
            <Thickness>0.02</Thickness>
            <VertexCentered/>
         </SlicingCircles>
      </Puzzle>
      <Edge/>
      <Puzzle>
         <SlicingCircles>
            <EdgeCentered>
               <Distance>0.75:0:0:0</Distance>
            </EdgeCentered>
            <FaceCentered/>
            <Thickness>0.02</Thickness>
            <VertexCentered/>
         </SlicingCircles>
      </Puzzle>
      <Vertex/>
      <Puzzle>
         <SlicingCircles>
            <EdgeCentered/>
            <FaceCentered/>
            <Thickness>0.02</Thickness>
            <VertexCentered>
               <Distance>0:0:1:0</Distance>
            </VertexCentered>
         </SlicingCircles>
      </Puzzle>
   </Specific>
   <TileShrink>0.94</TileShrink>
</PuzzleConfigClass>
